{
  "servers": {
    "github-enterprise-mcp": {
      "command": "uvx",
      "args": [
        "github-enterprise-mcp"
      ],
      "env": {
        "GITHUB_CONNECTION_NAME": "github-public1",
        "GITHUB_URL": "github.com",
        "GITHUB_TOKEN": "YOUR_GITHUB_TOKEN_HERE"
      }
    }
  }
}